{
  "kind": "collectionType",
  "collectionName": "issues",
  "info": {
    "singularName": "issue",
    "pluralName": "issues",
    "displayName": "Publication Issue",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date_published": {
      "type": "datetime",
      "pluginOptions": {}
    },
    "color": {
      "type": "customField",
      "customField": "plugin::color-picker.color"
    },
    "photos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::photo.photo"
    },
    "printed_copies": {
      "type": "integer",
      "private": true,
      "min": 100,
      "default": 2000
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}

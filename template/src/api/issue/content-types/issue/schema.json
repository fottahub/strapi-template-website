{
  "kind": "collectionType",
  "collectionName": "issues",
  "info": {
    "singularName": "issue",
    "pluralName": "issues",
    "displayName": "Publication Issue",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pub_date": {
      "type": "datetime",
      "pluginOptions": {},
      "unique": true
    },
    "color": {
      "type": "customField",
      "customField": "plugin::color-picker.color"
    },
    "printed_copies": {
      "type": "integer",
      "private": true,
      "min": 100,
      "default": 2000
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "period": {
      "type": "string"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "publication_issue"
    },
    "pub_volume": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::volume.volume",
      "inversedBy": "pub_issues"
    },
    "photos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::photo.photo",
      "mappedBy": "pub_issue"
    }
  }
}
